#!/bin/bash
# MPA Mobile App - Production Environment Configuration

# API Configuration
EXPO_PUBLIC_API_URL=https://api.mpaverification.com
EXPO_PUBLIC_API_TIMEOUT=30000

# App Configuration
EXPO_PUBLIC_APP_NAME=MPA Biometric Verification
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_BUILD_NUMBER=1

# Feature Flags
EXPO_PUBLIC_ENABLE_BIOMETRIC=true
EXPO_PUBLIC_ENABLE_CAMERA=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_SYNC=true

# Logging
EXPO_PUBLIC_LOG_LEVEL=info
EXPO_PUBLIC_ENABLE_REMOTE_LOGGING=true

# Security
EXPO_PUBLIC_ENABLE_SSL_PINNING=true
EXPO_PUBLIC_CERTIFICATE_PINNING=true
